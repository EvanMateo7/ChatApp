{
  "name": "chat",
  "version": "1.0.0",
  "description": "Chat using SocketIO",
  "main": "server.js",
  "scripts": {
    "test": "npx mocha -r ts-node/register --experimental-modules src/**/*.test.ts --timeout 10000 --exit",
    "watchServer": "tsc --watch",
    "watchClient": "webpack --config webpack.dev.js",
    "dev": "concurrently --kill-others \"npm run watchServer\" \"npm run watchClient\"",
    "build": "webpack --config webpack.prod.js",
    "start": "start http://localhost:3000 && nodemon --watch server/server server/server/server.js"
  },
  "keywords": [
    "websocket"
  ],
  "author": "Evan",
  "license": "ISC",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.35",
    "@fortawesome/free-regular-svg-icons": "^5.15.3",
    "@fortawesome/free-solid-svg-icons": "^5.15.3",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@types/express": "^4.17.11",
    "@types/node": "^15.0.2",
    "@types/socket.io": "^2.1.13",
    "date-fns": "^2.21.3",
    "express": "^4.17.1",
    "firebase": "^8.5.0",
    "firebase-admin": "^9.7.0",
    "formik": "^2.2.7",
    "image-type": "^4.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "socket.io": "^4.0.2",
    "socket.io-client": "^4.0.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.3",
    "concurrently": "^6.1.0",
    "fork-ts-checker-webpack-plugin": "^6.2.6",
    "mocha": "^9.0.3",
    "proxyquire": "^2.1.3",
    "source-map-loader": "^2.0.1",
    "ts-loader": "^9.1.2",
    "ts-node": "^10.2.0",
    "typescript": "^4.2.4",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  }
}
